
body
  background #dec
  color #000
  padding 0
  margin 0
  font-family "Ubuntu", sans-serif
  text-shadow 0 1px 0 #fff

h1, h2
  font-family: 'Ubuntu', sans-serif;
  font-style: italic;
  font-weight: bold;
  margin: 0


.user img
  width 20px
  height 20px
  margin-right 5px
  vertical-align -20%


a
a:visited
  color #370
  text-decoration none

a:hover
  color #000
  text-shadow 0 0 4px #6f1

a:active
  color #684

a.disabled
a.disabled:hover
a.disabled:active
a.disabled:visited
  color #777
  text-shadow inherit
  cursor default

.frame3d a
  color #03f
  text-decoration underline

.frame3d:-webkit-full-screen
  width 100%
  height 100%

.hidden
  display none

